{
    "name": "kinedu/cfdi-xslt",
    "description": "Librería para descargar los XSLT de la Cadena Original del Servicio de Administración Tributaria (SAT)",
    "homepage": "https://github.com/Kinedu/cfdi-xslt",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "Kinedu",
        "cfdi",
        "sat"
    ],
    "authors": [
        {
            "name": "Orlando Charles",
            "email": "orlando@kinedu.com",
            "homepage": "https://orlandocharles.com/",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.5"
    },
    "autoload": {
        "psr-4": {
            "Kinedu\\CfdiXslt\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Kinedu\\CfdiXslt\\Tests\\": "tests"
        }
    },
    "prefer-stable": true
}
